{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"build": {
			"outputs": ["dist/**", ".next/**"],
			"dependsOn": ["^build"]
		},
		"build-storybook": {
			"outputs": ["dist/storybook/**"],
			"dependsOn": ["^build"]
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": ["coverage/**"],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"]
		},
		"test:e2e": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"deploy": {
			"dependsOn": ["build", "test", "format", "lint"],
			"outputs": []
		},
		"generate": {
			"dependsOn": ["^generate"]
		},
		"db:migrate:deploy": {
			"outputs": []
		},
		"db:push": {
			"outputs": []
		},
		"db:seed": {
			"outputs": []
		}
  },
  "globalDependencies": [".env", "tsconfig.json", "jest.config.js"]
}
