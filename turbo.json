{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"build": {
			"outputs": ["dist/**", ".next/**"],
			"dependsOn": ["^build"]
		},
		"build-storybook": {
			"outputs": ["dist/storybook/**"],
			"dependsOn": ["^build"]
		},
		"lint": {
			"outputs": []
		},
		"deploy": {
			"dependsOn": ["build", "format", "lint"],
			"outputs": []
		},
		"generate": {
			"dependsOn": ["^generate"]
		},
		"db:migrate:deploy": {
			"outputs": []
		},
		"db:push": {
			"outputs": []
		},
		"db:seed": {
			"outputs": []
		}
	},
	"globalDependencies": [".env", "tsconfig.json"]
}
