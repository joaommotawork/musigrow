/*
  This is tsconfig is not used for compilation. It is here to support sonarqube.
*/
{
	"extends": "../../packages/tsconfig/base.json",
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "cypress"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"@models/*": ["src/models/*"],
			"@routes/*": ["src/routes/*"]
		}
	}
}
